{
  "functions": {
    "source": "dist/functions",
    "predeploy": [
      "npm run clean",
      "npm run build-funcs",
      "npm run build-app",
      "npm run copy-deps",
      "npm run install-deps"
    ]
  },
  "hosting": {
    "public": "dist/public",
    "rewrites": [
      {
        "source": "**/**",
        "function": "heroes-ssr"
      }
    ],
    "predeploy": "npm run build-public"
  }
}
